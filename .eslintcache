</div>\n    </div>\n  );\n});\n\nconst ControlOption = ({ value, clickOption, isSelect }) => {\n  const className = [\"control-item-option\", isSelect && \"option-active\"]\n    .filter((a) => a)\n    .join(\" \");\n  const style = isSelect\n    ? { background: isSelect, border: `1px solid ${isSelect}` }\n    : {};\n  const value_format = value === \"Usa\" ? \"USA\" : value;\n  return (\n    <div className={className} onClick={() => clickOption(value)} style={style}>\n      {value_format}\n    </div>\n  );\n};\n","/home/mintian/pku/vis/paper_management_system/src/Pages/AnalysisPage/TagView-Hor/index.js",[],"/home/mintian/pku/vis/paper_management_system/src/Components/TimeLine/index.js",["141","142","143","144","145"],"/home/mintian/pku/vis/paper_management_system/src/Hooks/index.js",[],"/home/mintian/pku/vis/paper_management_system/src/Pages/AnalysisPage/TagView-Hor/TagFilter/index.js",["146","147","148"],"/home/mintian/pku/vis/paper_management_system/src/Pages/AnalysisPage/ProjectView/index.js",["149","150"],"/home/mintian/pku/vis/paper_management_system/src/Pages/AnalysisPage/TopicView/index.js",[],"/home/mintian/pku/vis/paper_management_system/src/Pages/AnalysisPage/NetworkView/index.js",["151","152","153","154"],{"ruleId":"155","replacedBy":"156"},{"ruleId":"157","replacedBy":"158"},{"ruleId":"155","replacedBy":"159"},{"ruleId":"157","replacedBy":"160"},{"ruleId":"155","replacedBy":"161"},{"ruleId":"157","replacedBy":"162"},{"ruleId":"155","replacedBy":"163"},{"ruleId":"157","replacedBy":"164"},{"ruleId":"155","replacedBy":"165"},{"ruleId":"157","replacedBy":"166"},{"ruleId":"167","severity":1,"message":"168","line":14,"column":6,"nodeType":"169","endLine":14,"endColumn":8,"suggestions":"170"},{"ruleId":"155","replacedBy":"171"},{"ruleId":"157","replacedBy":"172"},{"ruleId":"167","severity":1,"message":"168","line":18,"column":6,"nodeType":"169","endLine":18,"endColumn":8,"suggestions":"173"},{"ruleId":"155","replacedBy":"174"},{"ruleId":"157","replacedBy":"175"